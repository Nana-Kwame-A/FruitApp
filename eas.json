{
  "build": {
    "base": {
      "node": "18.18.0",
      "yarn": "1.22.5"
    },
    "development": {
      "extends": "base",
      "channel": "development",
      "developmentClient": true,
      "android": {
        "distribution": "internal",
        "withoutCredentials": true
      },
      "ios": {
        "simulator": true
      }
    },
    "staging": {
      "extends": "base",
      "channel": "staging",
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "extends": "base",
      "channel": "production",
      "distribution": "store"
    },
    "preview": {
      "extends": "base",
      "channel": "preview",
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    }
  },
  "cli": {
    "version": ">= 0.48.2",
    "requireCommit": false,
    "appVersionSource": "remote"
  }
}
